{
  "presets": [ "react", "es2015" ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-class-properties",
    "transform-object-rest-spread",
    [
      "babel-plugin-transform-builtin-extend",
      {
        "globals": ["Array", "Error"]
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        "idx"
      ]
    }
  }
}
