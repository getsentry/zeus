    "webpack-cli": "^3.3.10",
    "webpack-manifest-plugin": "^2.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "webapp/**/*.{js,jsx}"
    ],
    "coverageDirectory": ".artifacts/coverage/",
    "coverageReporters": [
      "lcov",
      "cobertura"
    ],
    "forceCoverageMatch": [
      "**/*.jsx?"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/async.js",
      "<rootDir>/config/jest/date.js",
      "<rootDir>/config/jest/enzyme.js",
      "<rootDir>/config/jest/testStubs.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/webapp/**/__tests__/**/*.js?(x)",
      "<rootDir>/webapp/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/(node_modules)/?!(react-syntax-highlighter)"
    ]
  },
  "jest-junit": {
    "ancestorSeparator": " â€º ",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "usePathForSuiteName": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "globals": {
      "TestStubs": true,
      "tick": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/no-deprecated": "off",
      "react/prop-types": "off"
    }
  },
  "volta": {
    "node": "8.11.3",
    "yarn": "1.17.3"
  }
}
