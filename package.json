    "react-document-title": "^2.0.3",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.9",
    "react-hljs": "^1.0.0",
    "react-icon-base": "^2.0.8",
    "react-redux": "^5.0.5",
    "react-router": "2",
    "react-transition-group": "^2.2.0",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0",
